                </com.zhihu.matisse.internal.ui.widget.SquareFrameLayout>

                <com.zhihu.matisse.internal.ui.widget.SquareFrameLayout
                    android:layout_width="0dp"
                    android:layout_weight="1"
                    android:layout_height="match_parent">

                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent">

                        <ImageView
                            android:id="@+id/imgFiveTypeFive"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:contentDescription="@string/default_image"
                            android:scaleType="centerCrop" />

                        <ImageView
                            android:id="@+id/imgFiveTypeFivePlay"
                            android:layout_width="@dimen/size_30"
                            android:layout_height="@dimen/size_30"
                            android:layout_centerInParent="true"
                            android:src="@drawable/play_video"
                            android:visibility="gone"
                            tools:ignore="ContentDescription" />

                        <vn.techres.line.widget.TechResTextView
                            android:id="@+id/tvMoreImage"
                            android:layout_width="fill_parent"
                            android:layout_height="fill_parent"
                            android:background="@color/gray_background_transparent"
                            android:gravity="center"
                            android:textColor="@color/white"
                            android:textSize="@dimen/size_25"
                            android:visibility="visible"
                            tools:ignore="SpUsage" />
                        <ImageView
                            android:id="@+id/imgDeleteFiveFive"
                            android:layout_width="@dimen/size_25"
                            android:layout_height="@dimen/size_25"
                            android:src="@drawable/ic_close"
                            android:layout_margin="@dimen/margin_10dp"
                            android:layout_alignParentEnd="true"
                            android:visibility="gone"/>
                    </RelativeLayout>
                </com.zhihu.matisse.internal.ui.widget.SquareFrameLayout>
            </LinearLayout>
